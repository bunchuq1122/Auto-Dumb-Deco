{
	"geode": "5.2.0",
	"gd": {
		"win": "2.2081",
		"android": "2.2081"
	},
	"id": "bunchuq1122.auto-dumb-deco",
	"name": "Auto Dumb Deco",
	"version": "v1.1.1",
	"developer": "Bunchuq1122",
	"description": "Automatically decorates with RANDOM OBJECTS!",
	"settings": {
		"random_offset": {
			"type": "bool",
			"name": "random offset",
			"description": "Decorates with offset decorations",
			"default": false
		},
		"random_rotation": {
			"type": "bool",
			"name": "random rotation",
			"description": "Decorates with randomly rotated decorations",
			"default": false
		},
		"Layer": {
			"type": "int",
			"name": "Layer",
			"description": "The editor layer of decorations",
			"default": 1
		},
		"true_random": {
			"type": "bool",
			"name": "true random",
			"description": "allow Portals, Pad, and etc",
			"default": false
		},
		"random_color": {
			"type": "bool",
			"name": "random color",
			"description": "Decorates with random colors",
			"default": false
		},
		"random_color_range": {
			"type": "int",
			"name": "random color range",
			"description": "range of randomized color",
			"default": 1,
			"enable-if": "random_color",
			"max": 100,
			"min": 1
		},
		"random_scale": {
			"type": "bool",
			"name": "random scale",
			"description": "Decorates with randomly sized decorations",
			"default": false
		}
	},
	"tags": [
		"joke", "editor"
	],
	"links": {
		"community": "https://github.com/bunchuq1122/Auto-Dumb-Deco",
